<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.hand.POJO.Store" table="STORE">
      <meta attribute="class-description">
         This class contains the Store detail. 
      </meta>
      <id name="store_id" type="int" column="store_id">
         <generator class="native"/>
      </id>
      <property name="manager_stadd_id" column="manager_stadd_id" type="int"/>
      <property name="address_id" column="address_id" type="int"/>
   </class>
</hibernate-mapping>